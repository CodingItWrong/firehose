<h1>Add Link</h1>

<form onsubmit={{action 'handleSave'}}>
  <div class="form-group">
    <label for="url">URL</label>
    {{input data-test-url value=url id="url" type="url" classNames="form-control"}}
  </div>

  <button data-test-save-button type="submit" class="btn btn-primary">Save</button>
  {{#link-to 'index' classNames="btn btn-default"}}Cancel{{/link-to}}
</form>
